package{	import flash.display.Sprite;	import flash.display.MovieClip;	import flash.events.MouseEvent;	import Camera1;		public class KidTV1 extends Sprite	{		var cam1:Camera1;				public function KidTV1():void		{			tab1_mc.addEventListener(MouseEvent.CLICK, onTabClick);			tab2_mc.addEventListener(MouseEvent.CLICK, onTabClick);			tab3_mc.addEventListener(MouseEvent.CLICK, onTabClick);			tab4_mc.addEventListener(MouseEvent.CLICK, onTabClick);			tab5_mc.addEventListener(MouseEvent.CLICK, onTabClick);						cam1 = new Camera1();			cam1.x = -320;			cam1.y = -240;			video_mc.holder_mc.addChild(cam1);		}				private function onTabClick(evt:MouseEvent):void		{			var _mc:MovieClip = evt.currentTarget as MovieClip;						setChildIndex(_mc, numChildren - 1);			setChildIndex(video_mc, numChildren - 1);		}	}}