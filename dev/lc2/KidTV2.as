package{	import flash.display.Sprite;	import flash.display.MovieClip;	import flash.events.MouseEvent;	import Camera2;		public class KidTV2 extends Sprite	{		var cam2:Camera2;				public function KidTV2():void		{			tab1_mc.addEventListener(MouseEvent.CLICK, onTabClick);			tab2_mc.addEventListener(MouseEvent.CLICK, onTabClick);			tab3_mc.addEventListener(MouseEvent.CLICK, onTabClick);			tab4_mc.addEventListener(MouseEvent.CLICK, onTabClick);			tab5_mc.addEventListener(MouseEvent.CLICK, onTabClick);						cam2 = new Camera2();			video_mc.holder_mc.addChild(cam2);		}				private function onTabClick(evt:MouseEvent):void		{			var _mc:MovieClip = evt.currentTarget as MovieClip;						setChildIndex(_mc, numChildren - 1);			setChildIndex(video_mc, numChildren - 1);		}	}}